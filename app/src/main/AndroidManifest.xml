<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.popfun.sgsg.sgsg_project" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >

        <!--Login-->
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <!--Join-->
        <activity android:name=".JoinActivity"
            android:label="JoinActivity">
        </activity>


        <!--Main-->
        <activity android:name=".MainActivity"
            android:label="MainActivity">
        </activity>

        <!--개인화면 Individual-->
        <activity android:name=".IndividualActivity"
            android:label="IndividualActivity">
        </activity>

        <!--스터디그룹 StudyGroup-->
        <activity android:name=".StudyGroupActivity"
            android:label="StudyGroupActivity">
        </activity>

    </application>

</manifest>
